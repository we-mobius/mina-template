{
  "name": "mina-template",
  "version": "0.1.0",
  "description": "Mobius flavor MINA project templates.",
  "type": "module",
  "typings": "./typings/main.d.ts",
  "types": "./typings/main.d.ts",
  "main": "./release/modules/cjs/main.cjs",
  "module": "./release/modules/esm/main.js",
  "exports": {
    ".": {
      "require": "./release/modules/cjs/main.cjs",
      "import": "./release/modules/esm/main.js",
      "node": "./release/modules/esm/main.js",
      "default": "./release/modules/esm/main.js"
    },

    "./es": "./release/modules/es/main.js",
    "./es/*": "./release/modules/es/*",
    "./es-js/*": "./release/modules/es/*.js",

    "./ts": "./src/main.ts",
    "./ts/*": "./src/ts/*.ts",
    "./ts-js/*": "./src/ts/*.js",

    "./esm": "./release/modules/esm/main.js",
    "./umd": {
      "require": "./release/modules/umd/main.cjs",
      "import": "./release/modules/umd/main.js",
      "node": "./release/modules/umd/main.js",
      "default": "./release/modules/umd/main.js"
    },
    "./cjs": "./release/modules/cjs/main.cjs",

    "./src/*": "./src/*",
    "./src-ts/*": "./src/*.ts",
    "./src-js/*": "./src/*.js",
    "./statics/*": "./statics/*",
    "./release/*": "./release/*",
    "./release-js/*": "./release/*.js",
    "./release-cjs/*": "./release/*.cjs",
    "./esm/*": "./release/modules/esm/*",
    "./cjs/*": "./release/modules/cjs/*",
    "./umd/*": "./release/modules/umd/*",

    "./css": "./src/statics/styles/release.css",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "es": [
        "./release/modules/es/main.d.ts"
      ],
      "es/*": [
        "./release/modules/es/*"
      ],
      "es-js/*": [
        "./release/modules/es/*"
      ],
      "ts": [
        "./src/main.ts"
      ],
      "ts/*": [
        "./src/ts/*"
      ],
      "ts-js/*": [
        "./src/ts/*"
      ],
      "esm": [
        "./typings/main.d.ts"
      ],
      "umd": [
        "./typings/main.d.ts"
      ],
      "cjs": [
        "./typings/main.d.ts"
      ],
      "*": [
        "./typings/main.d.ts"
      ]
    }
  },
  "keywords": [
    "Mobius",
    "MINA"
  ],
  "directories": {
    "doc": "./docs",
    "bin": "./bin"
  },
  "scripts": {
    "cz-commit": "npm run lint && npx git-cz",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "dev": "node ./scripts/dev",
    "dev:web": "node ./scripts/dev type=web",
    "dev:mina": "node ./scripts/dev type=mina",
    "build": "node ./scripts/build",
    "build:web": "node ./scripts/build type=web",
    "build:mina": "node ./scripts/build type=mina",
    "dist": "node ./scripts/dist",
    "dist:web": "node ./scripts/dist type=web",
    "dist:mina": "node ./scripts/dist type=mina",
    "serve:build": "npx live-server ./build/web --port=0624",
    "serve:dist": "npx live-server ./dist/web --port=0624",
    "expose": "npx ngrok http 624",
    "bundle": "node ./scripts/bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/we-mobius/mina-template.git"
  },
  "author": {
    "name": "kongxiangyan",
    "email": "kcigaret@outlook.com",
    "url": "https://www.cigaret.world/"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/we-mobius/mina-template/issues"
  },
  "homepage": "https://github.com/we-mobius/mina-template#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@commitlint/cli": "^8.3.5",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.5",
    "commitlint-config-cz": "^0.13.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "cz-customizable": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.0.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mini-css-extract-plugin": "^1.3.3",
    "mini-svg-data-uri": "^1.1.3",
    "postcss": "^8.2.7",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-csso": "^5.0.0",
    "postcss-extend-rule": "^3.0.0",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^1.2.1",
    "tailwindcss": "^2.0.3",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.0",
    "webpack": "^5.40.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "jsdelivr": "./release/modules/umd/main.js",
  "files": [
    "/release/*",
    "/dist/*",
    "/build/*",
    "/src/*",
    "/public/*",
    "/docs/*",
    "/bin/*",
    "/typings/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": [
    "**/*.css",
    "./statics/**"
  ],
  "dependencies": {
    "lit-html": "^1.3.0"
  }
}
